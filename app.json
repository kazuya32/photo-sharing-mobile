{
  "expo": {
    "name": "FLEGO",
    "description": "Lego with Fun",
    "slug": "photo-sharing-mobile",
    "privacy": "unlisted",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon-ios.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.flego.ios",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "FLEGOがあなたのお気に入りのフォトや受け取ったデジタルサインフォトを保存することを許可します。",
        "NSPhotoLibraryUsageDescription": "FLEGOがカメラロールにアクセスすることとお気に入りの写真やデジタルサイン付きフォトを保存することを許可します。"
      }
    },
    "android": {
      "package": "com.flego.android",
      "versionCode": 5,
      "icon": "./assets/icon-android.png"
    },
    "facebookScheme": "fb2113813658871952",
    "facebookAppId": "2113813658871952",
    "facebookDisplayName": "FLEGO"
  }
